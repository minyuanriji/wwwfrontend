/*优惠券底部选择弹层*/
.use-points {
	background: #FFFFFF;
	margin-top: 40rpx;
	border-radius: 20rpx;
	padding: 10rpx 30rpx;
	font-size: 26rpx;
	color: #000000;
}
.points-switch {
	-webkit-transform: scale(0.7);
	        transform: scale(0.7);
}


.group-item-center {
  margin-bottom: 32rpx;
}
.list-root {
  padding-top: 20rpx;
}
.list-box {
  background: #FFFFFF;
  padding: 0 30rpx;
}
.list-items {
  padding: 24rpx 0;
  border-bottom: 1px solid #F2F2F2;
}
.list-items .list-item-left {
  width: 218rpx;
  height: 218rpx;
  border-radius: 10rpx;
  margin-right: 24rpx;
}
.list-items .list-item-right {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  padding: 6rpx 0;
}
.list-items .list-item-right .pro-name {
  font-size: 32rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #333333;
  line-height: 48rpx;
}
.list-items .list-item-right .pro-detail .group-start {
  height: 30rpx;
  margin-left: 4rpx;
  margin-bottom: 8rpx;
  font-size: 20rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  line-height: 48rpx;
  padding: 4rpx 8rpx;
  background: rgba(248, 221, 221, 0.3);
  border-radius: 0rpx 4rpx 4rpx 0rpx;
}
.list-items .list-item-right .pro-detail .people-num {
  width: 80rpx;
  height: 30rpx;
  letter-spacing: 2rpx;
  margin-bottom: 2rpx;
  font-size: 20rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #FFFFFF;
  border-radius: 4rpx 0rpx 0rpx 4rpx;
}
.list-items .list-item-right .pro-detail .pro-detail-bottom {
  font-size: 24rpx;
}
.list-items .list-item-right .pro-detail .pro-detail-bottom .price-box {
  margin-right: 14rpx;
}
.list-items .list-item-right .pro-detail .pro-detail-bottom .price-box .price {
  font-size: 40rpx;
  line-height: 48rpx;
}
.list-items .list-item-right .pro-detail .pro-detail-bottom .group-num {
  color: #999999;
  letter-spacing: 1px;
  -webkit-transform: scale(0.9);
          transform: scale(0.9);
  margin-top: 4rpx;
}
.list-items .list-item-right .pro-detail .pro-detail-bottom .start-btn-bottom {
  color: #FFFFFF;
  letter-spacing: 1px;
  text-align: center;
  width: 100rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: -webkit-linear-gradient(left, #D92F2F, #FB3939);
  background: linear-gradient(90deg, #D92F2F, #FB3939);
  border-radius: 6rpx;
}


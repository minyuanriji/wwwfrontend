
.orderBox{
	background: rgba(0,0,0,0.8);
	max-width: 500rpx;
	padding: 10rpx 16rpx 10rpx 10rpx;
	border-radius: 6rpx;
}
.avatar{
	width: 44rpx;
	height: 44rpx;
	border-radius: 50%;
	display: block;
	margin-right: 10rpx;
}
.msg{
	font-size: 9pt;
	color: white;
	white-space: nowrap;
	text-overflow: ellipsis;
	overflow: hidden;
}


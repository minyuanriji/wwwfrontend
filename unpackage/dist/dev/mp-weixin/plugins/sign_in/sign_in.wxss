.header {
  background: var(--textColor);
  height: 380rpx;
  padding: 52rpx 0 0 48rpx;
  color: #FFFFFF;
  box-sizing: border-box;
  position: relative;
}
.header .avatar {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
  margin-right: 28rpx;
}
.header .content {
  font-size: 34rpx;
}
.header .content .content-bottom {
  font-size: 24rpx;
  color: #fefefe;
  opacity: 0.65;
}
.header .integral {
  background: #FFFFFF;
  height: 66rpx;
  border-radius: 50rpx 0 0 50rpx;
  color: #000000;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  font-size: 36rpx;
  font-weight: 600;
}
.header .integral .integral-icon {
  background: var(--textColor);
  height: 46rpx;
  width: 46rpx;
  border-radius: 50%;
  margin-right: 18rpx;
}
/* 补签 */

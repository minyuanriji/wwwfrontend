}
.sign-modal-box .modal-head .modal-bg {
  width: 100%;
  height: 100%;
}
.sign-modal-box .modal-head .sign-tag {
  width: 70rpx;
  height: 70rpx;
  position: absolute;
  top: 50rpx;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
}
.sign-modal-box .modal-head .sign-num-box {
  font-size: 30rpx;
  color: #fff;

          transform: translateX(-50%);
}
.calendar {
  background: #fff;
  box-shadow: 2px 2px 9px 1px rgba(9, 63, 121, 0.15);
  border-radius: 10rpx;
  overflow: hidden;
}
.calendar .bar {
  height: 100rpx;
  font-size: 30rpx;
}
.calendar .bar .bar-child {
  width: 90%;
  background: #F3F2F2;
  border-radius: 60rpx;
  padding: 10rpx 24rpx;
}
.calendar .bar .bar-btn {
  background: none;
}
.calendar .bar .bar-btn .cuIcon-back,
.calendar .bar .bar-btn .cuIcon-right {
  color: #c4c4c4;
}
.calendar .bar .date {
  font-weight: 600;
  color: #000000;
}
.calendar .week .week-item {
  font-size: 24rpx;
  font-weight: 500;
  color: #999999;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.calendar .myDateTable {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
